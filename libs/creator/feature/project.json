{
  "projectType": "library",
  "sourceRoot": "libs/creator/feature/src",
  "prefix": "kudostories",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/creator/feature"
      ],
      "options": {
        "jestConfig": "libs/creator/feature/jest.config.ts",
        "passWithNoTests": true
      }
