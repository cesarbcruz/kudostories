    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/creator/feature/**/*.ts",
          "libs/creator/feature/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
